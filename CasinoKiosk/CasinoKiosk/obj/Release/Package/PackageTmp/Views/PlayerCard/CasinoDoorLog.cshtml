
@model List<CKDatabaseConnection.EF.sp_GetCasinoDoorLog_Result>

@using PagedList.Mvc;
@{

    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section HeaderContent
{
    <h2> CASINO DOOR LOG </h2>

}
<style>
    #printGolden:visited {
        pointer-events: none;
    }
</style>
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <form action="/PlayerCard/RegisterPlayer" method="post" onsubmit="submitForm(event)">
                    <div class="input-group col-md-12">
                        <label class="col-md-1">Date</label>
                        <input type="date" class="form-control col-md-2" id="fCardID" name="fCardID" onchange="changeFCardID()" required>
                        
                        <button class="btn btn-primary" type="submit" style="margin-left:100px" id="btnSearch" onclick="clickRegister()">
                            Search
                        </button>
                    </div>
                </form>

            </div>
            <br />
            <table id="datatable-json" class="table table-striped table-bordered table-hover">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>DeviceID</th>
                        <th>DoorID</th>
                        <th>CardNumber</th>
                        <th>PlayerID</th>
                        <th>PlayerName</th>
                        <th>GroupName</th>
                        <th>InOutState</th>
                        <th>isCardValid</th>

                        <th>isAllow</th>
                        <th>NotAllowReason</th>
                        <th>AccessTime</th>
                        <th>Success Code</th>
                        <th>ErrorCode</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.ID</td>
                            <td>@item.DeviceID</td>
                            <td>@item.DoorID</td>
                            <td>@item.CardNumber</td>
                            <td>@item.PlayerID</td>
                            <td>@item.PlayerName</td>
                            <td>@item.GroupName</td>
                            <td>@item.InOutState</td>
                            <td>@item.isCardValid</td>
                            <td>@item.isAllow</td>
                            <td>@item.NotAllowReason</td>
                            <td>@item.AccessTime</td>
                            <td>@item.isDoorOpenedSuccess</td>
                            <td>@item.DoorOpenedFailed_ErrorCode</td>
                        </tr>
                        
                    }
                </tbody>
            </table>

        </div>
    </div>
</div>
